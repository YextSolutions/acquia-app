        {
          "id": "acquiadrupal_acquiaCategory"
        },
        {
          "id": "yext/landingPageUrl"
        }
      ]
    },
    {
      "displayName": "Person Details",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/emails"
        },
        {
          "id": "acquiadrupal_jobTitle"
        },
        {
          "id": "yext/keywords"
        },
        {
          "id": "acquiadrupal_acquiaDescription"
        },
        {
          "id": "acquiadrupal_acquiaPlace"
        },
        {
          "id": "acquiadrupal_personType"
