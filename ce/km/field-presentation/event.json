{
  "$id": "ce_event",
  "$schema": "https://schema.yext.com/config/km/field-presentation/v1",
  "entityType": "yext/event",
  "displayName": "Event",
  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/name",
          "visibleOnAdd": true
        },
        {
          "id": "yext/categories",
          "visibleOnAdd": true
        },
        {
          "id": "yext/linkedLocation"
        },
        {
          "id": "yext/venueName"
        },
        {
          "id": "yext/address",
          "visibleOnAdd": true
        },
        {
          "id": "yext/timezone"
        },
        {
          "id": "yext/attendance"
        },
        {
          "id": "yext/eventStatus"
        },
        {
          "id": "yext/time",
          "visibleOnAdd": true
        },
        {
          "id": "yext/additionalPromotingLocations"
        },
        {
          "id": "yext/entityId",
          "visibleOnAdd": true
        },
        {
          "id": "c_doorTime"
        },
        {
          "id": "c_eventDuration"
        }
      ]
    },
    {
      "displayName": "Additional Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/description"
        },
        {
          "id": "c_acquiaDescription"
        },
        {
          "id": "yext/performers"
        },
        {
          "id": "yext/ageRange"
        },
        {
          "id": "yext/websiteUrl"
        },
        {
          "id": "yext/organizerName"
        },
        {
          "id": "yext/organizerEmail"
        },
        {
          "id": "yext/organizerPhone"
        },
        {
          "id": "yext/what3WordsAddress"
        }
      ]
    },
    {
      "displayName": "Tickets",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/isFreeEvent"
        },
        {
          "id": "yext/isTicketedEvent"
        },
        {
          "id": "yext/ticketUrl"
        },
        {
          "id": "yext/ticketAvailability"
        },
        {
          "id": "yext/ticketPriceRange"
        },
        {
          "id": "yext/ticketSaleDateTime"
        }
      ]
    },
    {
      "displayName": "Media",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/photoGallery"
        }
      ]
    },
    {
      "displayName": "Additional Attributes",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/keywords"
        }
      ]
    },
    {
      "displayName": "Landing Pages",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/landingPageUrl"
        }
      ]
    },
    {
      "displayName": "Other",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/labels"
        },
        {
          "id": "yext/folder"
        },
        {
          "id": "yext/deliverListingsWithoutGeocode"
        }
      ]
    }
  ]
}